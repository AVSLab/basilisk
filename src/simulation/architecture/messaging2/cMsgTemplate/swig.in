%{{
#include "{type}_C.h"
%}}
%include "{type}_C.h"
typedef struct {type};
%extend {type}_C {{
    %pythoncode %{{

    def subscribeTo(self, source):
        from Basilisk.simulation.messaging2 import {type}Class
        if type(source) == type(self):
            {type}_C_subscribe(self, source)
        elif type(source) == {type}Class:
            {type}_cpp_subscribe(self, source)
        else:
            raise Exception('tried to subscribe {type} to another message type')

    def log(self):
        from Basilisk.simulation.messaging2 import {type}Logger
        return {type}Logger(self)

    def userMessage(self, data=None):
        {type}_C_claim(self, self)
        if data:
            {type}_C_write(data, self)
        return self
    %}}
}};
